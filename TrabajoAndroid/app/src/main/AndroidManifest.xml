<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="afinal.edu.pe.trabajoandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".ClientsActivity" />
        <activity android:name=".ClientRegisterActivity" />
        <activity android:name=".ClientEditActivity" />
        <activity android:name=".ClientDetailActivity" />
        <activity android:name=".VehiclesActivity" />
        <activity android:name=".VehicleRegisterActivity" />
        <activity android:name=".VehicleEditActivity" />
        <activity android:name=".VehicleDetailActivity" />
        <activity android:name=".ServicesActivity" />
        <activity android:name=".ServiceOrdersActivity" />
        <activity android:name=".ServiceOrderRegisterActivity" />
        <activity android:name=".ServiceOrderEditActivity" />
        <activity android:name=".ServiceOrderDetailActivity"></activity>
    </application>

</manifest>